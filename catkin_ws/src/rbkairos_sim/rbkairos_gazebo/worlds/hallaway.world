        <collision name='Wall_7_Collision_1'>
          <geometry>
            <box>
              <size>6.68992 0.15 2.5</size>
            </box>
          </geometry>
          <pose>-0.125502 0 1.25 0 0 0</pose>
        </collision>
      </link>
      <static>1</static>
    </model>
    
    <actor name="actor">
  <skin>
    <filename>
      https://fuel.ignitionrobotics.org/1.0/Mingfei/models/actor/tip/files/meshes/walk.dae
    </filename>
    <scale>1.0</scale>
  </skin>
  <animation name="walk">
    <filename>
      https://fuel.ignitionrobotics.org/1.0/Mingfei/models/actor/tip/files/meshes/walk.dae
    </filename>
    <interpolate_x>true</interpolate_x>
  </animation>
  <collision>
    <geometry>
      <box>
        <size>1 1 1</size> <!-- Dimensioni del box -->
      </box>
    </geometry>
  </collision>
  <script>
    <loop>true</loop>
    <delay_start>0.000000</delay_start> 
    <auto_start>true</auto_start>
    <trajectory id="0" type="walk">
      <waypoint>
        <time>0</time>
        <pose>10 0 0 0 0 3.14159</pose>
      </waypoint>
      <waypoint>
        <time>20</time> <!-- Tempo in secondi per raggiungere il goal -->
        <pose>0 0 0 0 0 3.14159</pose> <!-- Pose del goal: x, y, z, roll, pitch, yaw -->
      </waypoint>
    </trajectory> 
  </script>
</actor>
    

    <state world_name='default'>
      <sim_time>0 0</sim_time>
      <real_time>0 0</real_time>
      <wall_time>0 0</wall_time>
      <iterations>0</iterations>
    </state>
    
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>18.82 -0.901146 5.07462 0 0.271643 -3.04699</pose>
        <view_controller>orbit</view_controller>
        <projection_type>perspective</projection_type>
      </camera>
    </gui>
  </world>
